{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": []
  },
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/app/app.component.ts",
    "src/main.ts"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
